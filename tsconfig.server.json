{
  "compilerOptions": {
    "outDir": "dist",
    "module": "CommonJS",
    "target": "ES2017",
    "strict": true,
    "moduleResolution": "Node",
    "lib": ["ESNext", "DOM", "ES2017"],
    "skipLibCheck": true,
    "isolatedModules": false,
    "esModuleInterop": true
  },
  "include": ["server/**/*"]
}
